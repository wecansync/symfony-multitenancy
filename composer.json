{
    "name": "wecansync/symfony-multi-tenancy-bundle",
    "version": "1.0.0",
    "description": "A multi tenancy symfony bundle to support multi-tenant system that works with any symfony project",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WeCanSync\\MultiTenancyBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "wecansync",
            "email": "info@wecansync.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "symfony/config": "^6.0|^6.1|^6.2|^6.3|^7.0",
        "symfony/dependency-injection": "^6.0|^6.1|^6.2|^6.3|^7.0",
        "symfony/http-kernel": "^6.0|^6.1|^6.2|^6.3|^7.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "symfony/orm-pack": "^2.0",
        "doctrine/annotations": "^1.0|^2.0",
        "symfony/filesystem": "^6.0|^7.0"
        
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
